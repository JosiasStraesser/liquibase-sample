{
    "databaseChangeLog": [
        {
            "preConditions": [
                {
                    "runningAs": {
                        "username": "liquibase"
                    }
                }
            ]
        },
        {
            "changeSet": {
                "id": "sqlFile-example",
                "author": "liquibase-docs",
                "changes": [
                    {
                        "sqlFile": {
                            "endDelimiter": "\\nGO",
